<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Boris Krit</Authors>
    <Description>Allows you to bind click commands to elements Xamarin Forms</Description>
    <Copyright>MIT</Copyright>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/scriptBoris/TouchSam</PackageProjectUrl>
    <RepositoryUrl>https://github.com/scriptBoris/TouchSam</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Xamarin, XamarinForms, Touch</PackageTags>
    <NeutralLanguage>en</NeutralLanguage>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Xamarin.Forms" Version="3.6.0.709228" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
